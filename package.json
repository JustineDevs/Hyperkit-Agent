{
  "name": "hyperkit",
  "version": "1.5.8",
  "description": "Web3 Development Platform with AI Generation and Cross-Chain DeFi Primitives",
  "main": "hyperkit-agent/cli/main.py",
  "scripts": {
    "version:current": "cat VERSION",
    "version:patch": "python hyperkit-agent/scripts/ci/version_bump.py patch",
    "version:minor": "python hyperkit-agent/scripts/ci/version_bump.py minor",
    "version:major": "python hyperkit-agent/scripts/ci/version_bump.py major",
    "version:update-docs": "python hyperkit-agent/scripts/ci/update_version_in_docs.py",
    "version:check": "python -c \"import json; pkg=json.load(open('package.json', encoding='utf-8')); ver=open('VERSION', encoding='utf-8').read().strip(); print(f'package.json: {pkg[\\\"version\\\"]}'); print(f'VERSION file: {ver}'); exit(0 if pkg['version']==ver else 1)\"",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "changeset:check": "changeset status --since HEAD~1",
    "release": "changeset publish",
    "install:root": "cd hyperkit-agent && pip install -e .",
    "install:dev": "cd hyperkit-agent && pip install -e \".[dev]\"",
    "install:all": "cd hyperkit-agent && pip install -e \".[dev,security]\"",
    "install:prod": "cd hyperkit-agent && pip install -e .",
    "build": "cd hyperkit-agent && python -m build",
    "publish:test": "cd hyperkit-agent && python -m twine upload --repository testpypi dist/*",
    "publish:prod": "cd hyperkit-agent && python -m twine upload dist/*",
    "clean": "cd hyperkit-agent && rm -rf dist/ build/ *.egg-info/",
    "test": "cd hyperkit-agent && python -m pytest tests/ -v",
    "test:coverage": "cd hyperkit-agent && python -m pytest --cov=core --cov=services --cov-report=term-missing tests/",
    "test:units": "cd hyperkit-agent && python -m pytest tests/unit/ -v",
    "test:integration": "cd hyperkit-agent && python -m pytest tests/integration/ -v",
    "test:e2e": "cd hyperkit-agent && python -m pytest tests/e2e/ -v",
    "lint": "cd hyperkit-agent && flake8 core/ services/",
    "format": "cd hyperkit-agent && black core/ services/",
    "format:check": "cd hyperkit-agent && black --check core/ services/",
    "type-check": "cd hyperkit-agent && mypy core/ services/",
    "security": "cd hyperkit-agent && bandit -r core/ services/",
    "security:contracts": "cd hyperkit-agent && slither contracts/",
    "docs:build": "cd hyperkit-agent && sphinx-build -b html docs/ docs/_build/html",
    "docs:serve": "cd hyperkit-agent && python -m http.server 8000 -d docs/_build/html",
    "hyperagent": "cd hyperkit-agent && python cli/main.py",
    "hyperagent:help": "cd hyperkit-agent && python cli/main.py --help",
    "hyperagent:status": "cd hyperkit-agent && python cli/main.py status",
    "hyperagent:version": "cd hyperkit-agent && python cli/main.py version",
    "hyperagent:test": "cd hyperkit-agent && python -m pytest tests/e2e/test_cli_commands_e2e.py -v",
    "hyperagent:test:all": "cd hyperkit-agent && python -m pytest tests/ -v",
    "hyperagent:audit": "cd hyperkit-agent && python cli/main.py audit --help",
    "hyperagent:deploy": "cd hyperkit-agent && python cli/main.py deploy --help",
    "hyperagent:generate": "cd hyperkit-agent && python cli/main.py generate --help",
    "hyperagent:workflow": "cd hyperkit-agent && python cli/main.py workflow --help",
    "hyperagent:monitor": "cd hyperkit-agent && python cli/main.py monitor --help",
    "hyperagent:config": "cd hyperkit-agent && python cli/main.py config --help",
    "hyperagent:verify": "cd hyperkit-agent && python cli/main.py verify --help",
    "hyperagent:batch-audit": "cd hyperkit-agent && python cli/main.py batch-audit --help",
    "hyperagent:test-rag": "cd hyperkit-agent && python cli/main.py test-rag --help",
    "hyperagent:limitations": "cd hyperkit-agent && python cli/main.py limitations",
    "docs:update": "npm run version:update-docs",
    "docs:audit": "cd hyperkit-agent && python scripts/maintenance/doc_drift_audit.py",
    "docs:cleanup": "cd hyperkit-agent && python scripts/maintenance/doc_drift_cleanup.py",
    "reports:organize": "echo REPORTS directory is already organized",
    "reports:status": "cd hyperkit-agent && python scripts/maintenance/cli_command_inventory.py",
    "reports:audit": "cd hyperkit-agent && python scripts/maintenance/legacy_file_inventory.py",
    "reports:todo": "cd hyperkit-agent && python scripts/maintenance/focused_todo_to_issues_conversion.py",
    "reports:compliance": "echo See hyperkit-agent/REPORTS/COMPLIANCE/ for compliance reports",
    "reports:quality": "echo See hyperkit-agent/REPORTS/QUALITY/ for quality reports",
    "reports:infrastructure": "echo See hyperkit-agent/REPORTS/STATUS/ for status reports",
    "diagnostics:cli": "cd hyperkit-agent && python scripts/diagnostics/cli_diagnostics.py"
  },
  "bin": {
    "hyperagent": "./hyperkit-agent/cli/main.py"
  },
  "keywords": [
    "web3",
    "defi",
    "smart-contracts",
    "ai",
    "hyperion",
    "andromeda",
    "blockchain",
    "ethereum",
    "cross-chain",
    "dapp",
    "sdk",
    "cli",
    "dashboard"
  ],
  "author": "HyperKit Team",
  "author_email": "team@hyperionkit.xyz",
  "license": "MIT",
  "homepage": "https://hyperionkit.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustineDevs/Hyperkit-Agent.git"
  },
  "bugs": {
    "url": "https://github.com/JustineDevs/Hyperkit-Agent/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.9"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1"
  },
  "files": [
    "hyperkit-agent/",
    "hyperkit-agent/core/",
    "hyperkit-agent/services/",
    "hyperkit-agent/cli/",
    "hyperkit-agent/tests/",
    "hyperkit-agent/scripts/",
    "hyperkit-agent/docs/",
    "docs/",
    "*.md",
    "*.txt",
    "*.yml",
    "*.yaml"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
