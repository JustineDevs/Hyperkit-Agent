{
  "name": "hyperkit",
  "version": "1.5.0",
  "description": "Web3 Development Platform with AI Generation and Cross-Chain DeFi Primitives",
  "main": "hypeagent/__init__.py",
  "scripts": {
    "version:current": "node -e \"console.log(require('./VERSION').toString().trim())\"",
    "version:show": "echo Current version: && type VERSION",
    "version:update": "python hyperkit-agent/scripts/update_version_in_docs.py",
    "version:patch": "python hyperkit-agent/scripts/version_bump.py patch",
    "version:minor": "python hyperkit-agent/scripts/version_bump.py minor", 
    "version:major": "python hyperkit-agent/scripts/version_bump.py major",
    "version:sync": "python hyperkit-agent/scripts/update_version_in_docs.py",
    "version:check": "echo Checking version consistency... && python -c \"import json; pkg=json.load(open('package.json', encoding='utf-8')); ver=open('VERSION', encoding='utf-8').read().strip(); print(f'package.json: {pkg[\\\"version\\\"]}'); print(f'VERSION file: {ver}'); print('Consistent' if pkg['version']==ver else 'Inconsistent')\"",
    "version:fix": "python hyperkit-agent/scripts/update_version_in_docs.py && echo Version sync complete",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "changeset:check": "changeset status --since HEAD~1",
    "version": "1.5.0",
    "release": "changeset publish",
    "test": "pytest tests/",
    "test:coverage": "pytest --cov=hyperkit tests/",
    "test:contracts": "pytest tests/contracts/",
    "test:modules": "pytest tests/modules/",
    "test:ai": "pytest tests/ai/",
    "lint": "flake8 hyperkit/",
    "format": "black hyperkit/",
    "format:check": "black --check hyperkit/",
    "type-check": "mypy hyperkit/",
    "security": "bandit -r hyperkit/",
    "security:contracts": "slither .",
    "install:dev": "pip install -e .[dev]",
    "install:prod": "pip install -e .",
    "build": "python -m build",
    "publish:test": "python -m twine upload --repository testpypi dist/*",
    "publish:prod": "python -m twine upload dist/*",
    "clean": "rm -rf dist/ build/ *.egg-info/",
    "docs": "sphinx-build -b html docs/ docs/_build/html",
    "docs:serve": "python -m http.server 8000 -d docs/_build/html",
    "dev": "python -m hyperkit.cli.main",
    "dashboard": "python -m hyperkit.dashboard.main",
    "init": "python -m hyperkit.cli.init",
    "generate": "python -m hyperkit.ai.generation",
    "deploy": "python -m hyperkit.contracts.deploy",
    "hyperagent": "cd hyperkit-agent && python cli/main.py",
    "hyperagent:help": "cd hyperkit-agent && python cli/main.py --help",
    "hyperagent:status": "cd hyperkit-agent && python cli/main.py status",
    "hyperagent:version": "cd hyperkit-agent && python cli/main.py version",
    "hyperagent:test": "cd hyperkit-agent && python -m pytest tests/e2e/test_cli_commands_e2e.py -v",
    "hyperagent:test:all": "cd hyperkit-agent && python -m pytest tests/ -v",
    "hyperagent:audit": "cd hyperkit-agent && python cli/main.py audit --help",
    "hyperagent:deploy": "cd hyperkit-agent && python cli/main.py deploy --help",
    "hyperagent:generate": "cd hyperkit-agent && python cli/main.py generate --help",
    "hyperagent:workflow": "cd hyperkit-agent && python cli/main.py workflow --help",
    "hyperagent:monitor": "cd hyperkit-agent && python cli/main.py monitor --help",
    "hyperagent:config": "cd hyperkit-agent && python cli/main.py config --help",
    "hyperagent:verify": "cd hyperkit-agent && python cli/main.py verify --help",
    "hyperagent:batch-audit": "cd hyperkit-agent && python cli/main.py batch-audit --help",
    "hyperagent:test-rag": "cd hyperkit-agent && python cli/main.py test-rag --help",
    "hyperagent:limitations": "cd hyperkit-agent && python cli/main.py limitations",
    "docs:update": "python hyperkit-agent/scripts/update_version_in_docs.py",
    "docs:audit": "python hyperkit-agent/scripts/doc_drift_audit.py",
    "docs:cleanup": "python hyperkit-agent/scripts/doc_drift_cleanup.py",
    "reports:organize": "echo REPORTS directory is already organized",
    "reports:status": "cd hyperkit-agent && python scripts/cli_command_inventory.py",
    "reports:audit": "cd hyperkit-agent && python scripts/legacy_file_inventory.py",
    "reports:todo": "cd hyperkit-agent && python scripts/focused_todo_to_issues_conversion.py",
    "reports:compliance": "echo See hyperkit-agent/REPORTS/COMPLIANCE/ for compliance reports",
    "reports:quality": "echo See hyperkit-agent/REPORTS/QUALITY/ for quality reports",
    "reports:status": "echo See hyperkit-agent/REPORTS/STATUS/ for status reports"
  },
  "bin": {
    "hyperkit": "./hypeagent/cli/main.py",
    "hyperkit-update": "./.github/workflows/scripts/update.js"
  },
  "keywords": [
    "web3",
    "defi",
    "smart-contracts",
    "ai",
    "hyperion",
    "andromeda",
    "blockchain",
    "ethereum",
    "cross-chain",
    "dapp",
    "sdk",
    "cli",
    "dashboard"
  ],
  "author": "HyperKit Team",
  "author_email": "team@hyperionkit.xyz",
  "license": "MIT",
  "homepage": "https://hyperionkit.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustineDevs/Hyperkit-Agent.git"
  },
  "bugs": {
    "url": "https://github.com/JustineDevs/Hyperkit-Agent/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.9"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1"
  },
  "files": [
    "hypeagent/",
    ".github/workflows/scripts/",
    "docs/",
    "tests/",
    "*.md",
    "*.txt",
    "*.yml",
    "*.yaml"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
