# HyperAgent Environment Configuration
# Copy this file to .env and fill in your actual values

# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LazAI Network Configuration
LAZAI_EVM_ADDRESS=0xa43b752b6e941263eb5a7e3b96e2e0dea1a586ff
LAZAI_RSA_PRIVATE_KEY=your_rsa_private_key_here
LAZAI_API_KEY=your_lazai_api_key_here

# Blockchain Configuration
DEFAULT_PRIVATE_KEY=your_private_key_here
DEFAULT_NETWORK=hyperion

# IPFS Storage Configuration
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here

# MCP Configuration
MCP_ENABLED=true

# MCP Docker Configuration (if using Docker)
MCP_DOCKER=false
MCP_HOST=localhost
MCP_PORT=3333

# Primary Network RPC URLs
HYPERION_RPC_URL=https://hyperion-testnet.metisdevops.link
LAZAI_RPC_URL=https://lazai-testnet-rpc.example.com
METIS_RPC_URL=https://andromeda.metis.io/?owner=1088

# Explorer API Keys (for primary networks)
HYPERION_EXPLORER_API_KEY=your_hyperion_explorer_api_key_here
METIS_EXPLORER_API_KEY=your_metis_explorer_api_key_here

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/hyperagent.log